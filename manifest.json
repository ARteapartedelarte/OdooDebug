{
    "name": "Odoo Debug",
    "version": "2.0",
    "manifest_version": 2,
    "description": "Toggle debug mode of Odoo webpage. Only work with Odoo",
    "browser_action": {
        "default_icon": "off.png",
        "default_title": "Click for Debug || Double Click for Assets Debug"
    },
    "incognito": "split",
    "background": {
        "scripts": ["jquery.min.js", "jquery.ba-bbq.min.js", "background.js"]
    },
    "permissions": ["activeTab", "webNavigation", "*://*/*", "tabs"],
    "icons": {
        "16": "off.png",
        "128": "off.png"
    }
}
